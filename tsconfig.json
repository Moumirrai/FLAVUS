{
  "compilerOptions": {
    "module": "CommonJS",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "rootDir": "src",
    "outDir": "dist",
    "skipLibCheck": true,
    "target": "ESNext",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    },
  },
  "include": ["src"]
}